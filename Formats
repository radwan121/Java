package Codes;

//Header file for java inputs
import java.util.Scanner;       //Scanner class is located in util class

public class Programs           //Anyone can access this class for public access modifires
{
    public static void main(String[] args) 
    {
        Scanner input = new Scanner(System.in);       //taking input
        String name;
        System.out.print("Enter name= ");
        name=input.nextLine();                //taking string input  

        System.out.println(name);//Printing
    }
}
